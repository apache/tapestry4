<!-- $Id$ -->

<specification>
	<class>tests.tapestry.Inspector</class>
	
	<parameters>
		<allow-informal-parameters>no</allow-informal-parameters>
	</parameters>
	
	<components>
		<component>
			<id>wrapped</id>
			<type>InsertWrapped</type>
		</component>
		
		<component>
			<id>ifVisible</id>
			<type>Conditional</type>
			
			<bindings>
				<binding>
					<name>condition</name>
					<property-path>visible</property-path>
				</binding>
			</bindings>
		</component>

		<component>
			<id>ifHidden</id>
			<type>Conditional</type>

			<bindings>
				<binding>
					<name>condition</name>
					<property-path>visible</property-path>
				</binding>
				
				<static-binding>
					<name>invert</name>
					<value>true</value>
				</static-binding>
			</bindings>
		</component>
		
		<component>
			<id>hideInspector</id>
			<type>Direct</type>
			
			<bindings>
				<binding>
					<name>listener</name>
					<property-path>hideInspectorListener</property-path>
				</binding>
				
				<static-binding>
					<name>anchor</name>
					<value>inspector</value>
				</static-binding>
			</bindings>
		</component>

		<component>
			<id>e-path</id>
			<type>Foreach</type>
			
			<bindings>
				<binding>
					<name>source</name>
					<property-path>componentPath</property-path>
				</binding>
				
				<binding>
					<name>value</name>
					<property-path>currentComponent</property-path>
				</binding>
			</bindings>
		</component>

		<component>
			<id>selectComponentId</id>
			<type>Direct</type>
			
			<bindings>
				<binding>
					<name>listener</name>
					<property-path>selectComponentListener</property-path>
				</binding>
				
				<binding>
					<name>context</name>
					<property-path>currentComponent.idPath</property-path>
				</binding>
				
				<static-binding>
					<name>anchor</name>
					<value>inspector</value>
				</static-binding>
			</bindings>
		</component>
		
		<component>
			<id>insertComponentId</id>
			<type>Insert</type>
			
			<bindings>
				<binding>
					<name>value</name>
					<property-path>currentComponent.id</property-path>
				</binding>
			</bindings>
		</component>

		<component>
			<id>e-child</id>
			<type>Foreach</type>
			
			<bindings>
				<binding>
					<name>source</name>
					<property-path>childComponentIds</property-path>
				</binding>
				
				<binding>
					<name>value</name>
					<property-path>childComponentId</property-path>
				</binding>
				
				<binding>
					<name>first</name>
					<property-path>first</property-path>
				</binding>
				
				<binding>
					<name>last</name>
					<property-path>last</property-path>
				</binding>
			</bindings>
		</component>

		<component>
			<id>ifFirstContained</id>
			<type>Conditional</type>
			
			<bindings>
				<binding>
					<name>condition</name>
					<property-path>first</property-path>
				</binding>
			</bindings>
			</component>
			
		<component>
			<id>ifLastContained</id>
			<type>Conditional</type>
			
			<bindings>
				<binding>
					<name>condition</name>
					<property-path>last</property-path>
				</binding>
			</bindings>
		</component>

		<component>
			<id>selectChild</id>
			<type>Direct</type>
			
			<bindings>
				<binding>
					<name>listener</name>
					<property-path>selectChildListener</property-path>
				</binding>
				
				<binding>
					<name>context</name>
					<property-path>childComponentIdPath</property-path>
				</binding>
				
				<static-binding>
					<name>anchor</name>
					<value>inspector</value>
				</static-binding>
			</bindings>
		</component>

		<component>
			<id>insertChildId</id>
			<type>Insert</type>
			
			<bindings>
				<binding>
					<name>value</name>
					<property-path>childComponentId</property-path>
				</binding>
			</bindings>
		</component>

