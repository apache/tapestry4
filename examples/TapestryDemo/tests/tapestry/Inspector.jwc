	<type>Conditional</type>
	<bindings>
	<binding>
	<name>condition</name>
	<property-path>last</property-path>
	</binding>
	</bindings>
	</component>

	<component>
	<id>insertPropertyName</id>
	<type>Insert</type>
	<bindings>
	<binding>
	<name>value</name>
	<property-path>pageChange.propertyName</property-path>
	</binding>
	</bindings>
	</component>

	<component>
