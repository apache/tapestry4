	<component>
	<id>e-bindings</id>
	<type>Foreach</type>
	<bindings>
	<binding>
	<name>source</name>
	<property-path>bindingNames</property-path>
	</binding>
	<binding>
	<name>value</name>
	<property-path>bindingName</property-path>
	</binding>
	<binding>
	<name>first</name>
	<property-path>first</property-path>
	</binding>
	<binding>
	<name>last</name>
	<property-path>last</property-path>
	</binding>
	</bindings>
	</component>

