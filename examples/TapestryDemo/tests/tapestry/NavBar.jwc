<?xml version="1.0"?>
<!DOCTYPE specification PUBLIC 
	"-//Primix Solutions//Tapestry Specification 1.0//EN"
	"http://tapestry.primix.com/dtd/Tapestry_1_0.dtd">
<!-- $Id$ -->

<specification>
	<class>tests.tapestry.NavBar</class>
	
	<allow-body>no</allow-body>
	
	<parameters>
		<allow-informal-parameters>no</allow-informal-parameters>
	</parameters>
	
	<components>
		<component>
			<id>e</id>
			<type>Foreach</type>
			
			<bindings>
				<binding>
					<name>source</name>
					<property-path>pageNames</property-path>
				</binding>
				
				<binding>
					<name>value</name>
					<property-path>currentPageName</property-path>
				</binding>
			</bindings>
		</component>
		
		<component>
			<id>pageLink</id>
			<type>Page</type>
			
			<bindings>
				<binding>
					<name>page</name>
					<property-path>currentPageName</property-path>
				</binding>

				<binding>
					<name>enabled</name>
					<property-path>pageLinkEnabled</property-path>
				</binding>
			</bindings>
		</component>
		
		<component>
			<id>insertPageName</id>
			<type>Insert</type>
			
			<bindings>
				<binding>
					<name>value</name>
					<property-path>displayName</property-path>
				</binding>
			</bindings>
		</component>	

		<component>
			<id>restartLink</id>
			<type>Service</type>
			
			<bindings>
				<static-binding>
					<name>service</name>
					<value>restart</value>
				</static-binding>
			</bindings>
		</component>
		
		<component>
			<id>resetLink</id>
			<type>Service</type>
			
			<bindings>
				<static-binding>
					<name>service</name>
					<value>reset</value>
				</static-binding>
			</bindings>
		</component>
	</components>
	
</specification>